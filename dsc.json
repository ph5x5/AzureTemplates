{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json",
	"contentVersion": "1.0.0.0",
	"parameters": {	
		"DSC" : {
			"type": "object"
		}
	},
	"variables": { },
	"resources": [
		{
			"apiVersion": "2015-06-15",
			"name": "Microsoft.Powershell",
			"type": "DSC",
			"location": "[parameters('DSC').Location]",
			"dependsOn": [ ],
			"tags": {
				"displayName": "Microsoft.Powershell.DSC"
			},
			"properties": {
				"publisher": "Microsoft.Powershell",
				"type": "DSC",
				"typeHandlerVersion": "2.20",
				"autoUpgradeMinorVersion": true,
				"settings": {
					"configuration": {
						"url": "[concat(parameters('DSC')._artifactsLocation, '/', variables('DSC').dscExtensionArchiveFolder, '/', variables('DSC').dscExtensionArchiveFileName)]",
						"script": "variables('DSC').dscExtensionArchiveFileName",
						"function": "variables('DSC').dscExtensionFunction"
					},
					"configurationArguments": {
							"bindPort": "[variables('DSC').dscIISBindPort]"
					}
				},
				"protectedSettings": {
					"configurationUrlSasToken": "[variables('DSC')._artifactsLocationSasToken]"
				}
			}
		}
	],
	"outputs": { }
}